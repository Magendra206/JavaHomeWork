package lambdademoo;

@FunctionalInterface
interface Cab
{
	public void bookCab();
}

class Ola implements Cab
{

	@Override
	public void bookCab() {
		System.out.println("Ola cab is Booked....");
		
	}
}

public class Lambda_First_Prg {

	public static void main(String[] args) {
		
		Ola o = new Ola();
		o.bookCab();
	}

}
