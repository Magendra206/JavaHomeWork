package lambdademoo;
@FunctionalInterface
interface Bike
{
	public String bookBike(String Source,String Destination);
}

public class Lambda_Return {

	public static void main(String[] args) {
		
		Bike b = (Source,Destination)->{
			System.out.println("Ola Bike is Booked from"+Source+" to"+Destination);
			return ("Price : 5000Rs");
		};
		b.bookBike("Hydrabad", "Bombay");
	}

}
